    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "blockchain",
    "cryptocurrency",
    "rust",
    "typescript",
    "sdk",
    "privacy",
    "zkp",
    "sharding",
    "wasm",
    "cross-chain",
    "ethereum",
    "did",
    "governance",
    "simulation"
  ],
  "author": "Gillean Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/gillean.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/gillean/issues"
  },
  "homepage": "https://github.com/yourusername/gillean#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "crypto-js": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@types/crypto-js": "^4.2.0",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
