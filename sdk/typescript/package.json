  "description": "TypeScript SDK for Gillean Blockchain - A privacy-focused, enterprise-grade blockchain platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
